@model HashSet<string>
@{
    ViewData["Title"] = "Index";
}

<form method="post" asp-action="Add" enctype="aplication/x-www-form-urlencoded">
    <input type="text" name="name" />
    <input type="number" name="score" />
    <button type="submit">Kaydet</button>
</form>

@foreach (var item in Model)
{
    <h2>@item <a asp-controller="SortedSetType" asp-action="DeleteItem" asp-route-name="@item">Sil</a></h2>
}